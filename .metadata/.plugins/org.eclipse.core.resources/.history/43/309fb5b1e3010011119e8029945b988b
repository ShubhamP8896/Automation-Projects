package practiceExam040226;

public class Reverse 
{
	 public static void main(String[] args) {
	        String string = "Hello";
	        String reversed = "";

	        for (int i = string.length() - 1; i >= 0; i--) {
	            reversed = reversed + str.charAt(i);
	        }

	        System.out.println("Reversed string: " + reversed);
	    }
}
