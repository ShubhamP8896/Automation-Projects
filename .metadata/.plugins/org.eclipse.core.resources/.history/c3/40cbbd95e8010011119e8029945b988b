package practiceExam040226;

public class ArrayProgram 
{
	public static void main(String[] args) 
	{
		 int[] a = {10, 25, 20, 30, 1};
		 
		 int max = a[0];
			
		 for(int i = 0; i<a.length; i++)
		 {
			 if(a[i] > max)
			 {
				 max = a[i];
			 }
		 }
		 System.out.println(max); 		
	}
	
	
	
	
}
